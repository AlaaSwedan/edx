ct-rest-spread":"^6.26.0","babel-preset-env":"^1.6.0","babel-preset-react":"^6.24.1","css-loader":"^0.28.5","eslint-config-edx":"4.0.0","extract-text-webpack-plugin":"^3.0.0","html-webpack-harddisk-plugin":"^0.1.0","html-webpack-plugin":"^2.30.1","husky":"^0.14.3","node-sass":"^4.5.3","react-dev-utils":"^4.0.0","sass-loader":"^6.0.6","style-loader":"^0.18.2","webpack":"^3.5.5","webpack-dev-server":"^2.7.1","webpack-merge":"^4.1.0"},"babel":{"presets":[["env",{"targets":{"browsers":["last 2 versions","ie 11"]}}],"babel-preset-react"],"plugins":["transform-object-rest-spread","transform-class-properties"]},"gitHead":"2af38d6bf13cc5c5002ab6f30fdf968a0e03a8eb","_id":"@edx/studio-frontend@0.1.1","_shasum":"b3ad2d0d22f9fb7bc31463ab1abd471815ed3487","_from":".","_npmVersion":"3.10.10","_nodeVersion":"6.11.3","_npmUser":{"name":"efischer19","email":"efischer0119@gmail.com"},"dist":{"shasum":"b3ad2d0d22f9fb7bc31463ab1abd471815ed3487","tarball":"https://registry.npmjs.org/@edx/studio-frontend/-/studio-frontend-0.1.1.tgz"},"maintainers":[{"email":"greg@edx.org","name":"yro"},{"email":"hrein@edx.org","name":"harryrein"},{"email":"rraposa@edx.org","name":"robrap"},{"email":"andya@edx.org","name":"andywarmstrong"},{"email":"aswan@edx.org","name":"alasdairswan"},{"email":"djen@edx.org","name":"djen"},{"email":"tyler@hallada.net","name":"thallada"},{"email":"mroytman@edx.org","name":"michaelroytman"},{"email":"efischer0119@gmail.com","name":"efischer19"},{"email":"arizzitano@gmail.com","name":"arizzitano"},{"email":"npmjs@edx.org","name":"edx-old-org"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/studio-frontend-0.1.1.tgz_1506024345225_0.9700464070774615"},"directories":{}}},"readme":"# studio-frontend\nReact front end for edX Studio\n\n## Development\n\nRequirements:\n* [Docker 17.06 CE+](https://docs.docker.com/engine/installation/), which should come with [docker-compose](https://docs.docker.com/compose/install/) built in.\n* A working, running [edX devstack](https://github.com/edx/devstack)\n\nTo install and run locally:\n```\n$ git clone git@github.com:edx/studio-frontend.git\n$ cd studio-frontend\n$ docker-compose -f docker-compose.yml up studio-frontend\n```\n\nTo install a new node package in the repo (assumes container is running):\n```\n$ make shell\n$ npm install <package> --save-dev\n$ exit\n$ git add package.json\n```\nTo make changes to the Docker image locally, modify the Dockerfile as needed and run:\n```\n$ docker build -t mroytman/studio-frontend:latest .\n```\n\nWebpack will serve pages in development mode at http://localhost:18011.\n\nNotes:\n\nThe development server will run regardless of whether devstack is running along side it. If devstack is not running, requests to the studio API will fail. You can start up the devstack at any time by following the instructions in the devstack repository, and the development server will then be able to communicate with the studio container. API requests will return the following statuses, given your current setup:\n\n| Studio Running? | Logged in?             | API return |\n|-----------------|------------------------|------------|\n| No              | n/a                    | 504        |\n| Yes             | No                     | 404        |\n| Yes             | Yes, non-staff account | 403        |\n| Yes             | Yes, staff account     | 200        |\n\n## Releases\n\nCurrently, the process for releasing a new version of our package is as follows:\n\n1. Make your changes in a PR and get them into master, as usual. Do not bump the version in package.json!\n2. After merging your changes, `git checkout master && git pull`. Ensure your current directory is cleaned, with no outstanding commits or files.\n3. Be a member of the correct edX and npm orgs, and be logged in. All of @edx/educator-dahlia should be set up, and others shouldn't need to be publishing this package.\n4. `make publish`. This will:\n  - verify that you're on a clean master branch\n  - bump the version in package.json, as a commit on your local master branch\n    - note that this will be a patch version bump by default; for other release types modify the `npm version patch` sub-command under the `publish` make target.\n  - tag that commit as a Github release tag\n  - `git checkout` to a new branch\n  - run our publish script to build everything and send it up to npm, and\n  - push all git changes up to the remote.\n5. Go view your newly created PR, and merge it. Also consider modifying the release tag with more descriptive release notes.\n6. If needed, send an email to release-notifications@edx.org. Summarize your change and link to:\n    - the github release\n    - the npm package\n","maintainers":[{"email":"greg@edx.org","name":"yro"},{"email":"hrein@edx.org","name":"harryrein"},{"email":"rraposa@edx.org","name":"robrap"},{"email":"andya@edx.org","name":"andywarmstrong"},{"email":"aswan@edx.org","name":"alasdairswan"},{"email":"djen@edx.org","name":"djen"},{"email":"tyler@hallada.net","name":"thallada"},{"email":"mroytman@edx.org","name":"michaelroytman"},{"email":"efischer0119@gmail.com","name":"efischer19"},{"email":"arizzitano@gmail.com","name":"arizzitano"},{"email":"npmjs@edx.org","name":"edx-old-org"}],"time":{"modified":"2017-09-21T20:05:47.333Z","created":"2017-09-06T18:45:49.020Z","0.0.1":"2017-09-06T18:45:49.020Z","0.1.0":"2017-09-21T14:30:34.181Z","0.1.1":"2017-09-21T20:05:47.333Z"},"license":"AGPL-3.0","readmeFilename":"README.md","_attachments":{},"_etag":"W/\"59c41b9b-2cfa\"","_lastModified":"Thu, 21 Sep 2017 20:05:47 GMT","_cached":true}